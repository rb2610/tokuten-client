{
  "name": "tokuten-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node --require dotenv/config node_modules/serve/bin/serve.js -s -p 3000 build",
    "dev": "node --require dotenv/config scripts/start.js",
    "build": "node scripts/build.js",
    "heroku-postbuild": "npm run build",
    "watch-tests": "tsc -w -p tsconfig.test.json",
    "test": "nyc mocha -r ts-node/register -r ignore-styles -r ./test/setupTests.ts 'test/**/*.test.{ts,tsx}' --reporter mochawesome --reporter nyan",
    "start-and-watch-test": "npm run build && npm run start & npm run watch-tests",
    "acceptance-test": "nightwatch"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@types/chai": "^4.1.6",
    "@types/enzyme": "^3.1.14",
    "@types/fixed-data-table-2": "^0.8.2",
    "@types/material-ui": "^0.21.5",
    "@types/mocha": "^5.2.5",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.2.0",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fixed-data-table-2": "^1.0.0-beta.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "nyc": "^13.1.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.5.2",
    "react-container-dimensions": "^1.4.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.2",
    "resolve": "1.6.0",
    "serve": "^10.0.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-loader": "^2.3.7",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/nightwatch": "^0.9.9",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "chromedriver": "^2.42.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "ignore-styles": "^5.0.1",
    "mock-http-server": "^1.1.0",
    "mockttp": "^0.11.0",
    "nightwatch": "^1.0.11",
    "typescript": "^3.1.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
